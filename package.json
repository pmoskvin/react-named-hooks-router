{
  "name": "react-named-hooks-router",
  "version": "1.1.4",
  "description": "Simple react router with hooks and named routes",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.6.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.5",
    "@types/mocha": "^5.2.7",
    "@types/power-assert": "^1.5.0",
    "child_process": "^1.0.2",
    "csstype": "^2.6.7",
    "fs-extra": "^8.1.0",
    "path": "^0.12.7",
    "prettier": "^1.18.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4",
    "ts-mocha": "^6.0.0",
    "mocha": "^6.2.1",
    "power-assert": "^1.6.1",
    "react": "^16.10.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "coveralls": "^3.0.6",
    "conventional-github-releaser": "^3.1.3"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "ts-mocha -p tsconfig.test.json src/**/*.test.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "coveralls": "npm run coverage && nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "github-release": "conventional-github-releaser"
  },
  "nyc": {
    "include": [
      "src/**/*.ts*"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.test.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "temp-directory": "./.cache/.nyc_output",
    "reporter": [
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmoskvin/react-named-hooks-router.git"
  },
  "keywords": [
    "react",
    "router",
    "routing",
    "route",
    "ts",
    "typescript"
  ],
  "author": {
    "name": "Peter Moskvin",
    "email": "peter.moskvin@outlook.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmoskvin/react-named-hooks-router/issues"
  },
  "homepage": "https://github.com/pmoskvin/react-named-hooks-router#readme"
}
